[root@server ~]# docker run -it --name mycontainer  ubuntu
root@1cc3158836af:/#
# Install apache server
>   apt-get update
>   apt-get install -y apache2 curl net-tools
>   service apache2 status
>   echo 'Hello World' > /var/www/html/index.html
>   service apache2 start
>   netstat -plnt
>   curl localhost
>   ifconfig
>   curl 172.17.0.2
>   exit



